{
	"name": "ResearchR",
	"version": "1.0",
	"manifest_version": 3,
	"icons": {
		"16": "icons/ResearchR Icon16x16.png",
		"32": "icons/ResearchR Icon32x32.png",
		"48": "icons/ResearchR Icon48x48.png",
		"128": "icons/ResearchR Icon128x128.png"
	},
	"action": {
		"default_popup": "popup/popup.html"
	},
	"permissions": [
		"history",
		"tabs",
		"storage"
	],
	"background":
	{
		"service_worker": "js/background.js"
	}
}